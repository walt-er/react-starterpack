{
  "name": "CHANGME",
  "description": "This is the description for a brand new project.",
  "version": "0.1.0",
  "authors": [
    "Walter Mitchell <walt@walt.codes>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/walt-er/CHANGEME",
  "repository": {
    "type": "git",
    "url": "https://github.com/walt-er/CHANGEME.git"
  },
  "bugs": {
    "url": "https://github.com/walt-er/CHANGEME/issues"
  },
  "scripts": {
    "start": "npm run vet && npm run watch",
    "watch": "webpack-dev-server --colors --no-inline --env.HOT",
    "build": "npm run vet && npm run build:webpack",
    "build:webpack": "webpack --progress --colors --env.BUILD",
    "stop": "sudo killall -9 node",
    "tkill": "taskkill /f /im node.exe",
    "stylelint": "stylelint ./src/styles/main.scss",
    "eslint": "eslint ./src/scripts/*/*.js ./src/templates/components/*/*.js",
    "vet": "npm run stylelint && npm run eslint"
  },
  "pre-commit": [
    "vet"
  ],
  "dependencies": {
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "copy-webpack-plugin": "^4.3.1",
    "css-hot-loader": "1.3.2",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "1.1.5",
    "node-sass": "^4.5.2",
    "node-sass-json-importer": "3.0.2",
    "path": "^0.12.7",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^3.0.0",
    "pre-commit": "1.1.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
