{
  "name": "CHANGME",
  "description": "This is the description for a brand new project.",
  "version": "0.1.0",
  "authors": [
    "Walter Mitchell <walt@walt.codes>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/walt-er/CHANGEME",
  "repository": {
    "type": "git",
    "url": "https://github.com/walt-er/CHANGEME.git"
  },
  "bugs": {
    "url": "https://github.com/walt-er/CHANGEME/issues"
  },
  "scripts": {
    "webpack:watch": "webpack-dev-server --colors --no-inline --env.HOT",
    "webpack:dev": "webpack --progress --colors --env.BUILD",
    "webpack:prod": "webpack --progress --colors --env.PROD",
    "stylelint": "stylelint ./src/styles/main.scss",
    "eslint": "eslint --ext .js --ext .jsx ./src/scripts/* ./src/components/*",
    "vet": "npm run stylelint && npm run eslint",
    "start": "npm run vet && npm run webpack:watch",
    "build:dev": "npm run vet && npm run webpack:dev",
    "build:prod": "npm run vet && npm run webpack:prod",
    "stop": "sudo killall -9 node",
    "tkill": "taskkill /f /im node.exe"
  },
  "pre-commit": [
    "vet"
  ],
  "dependencies": {
    "prop-types": "15.5.8",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "7.2.5",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "copy-webpack-plugin": "4.3.1",
    "css-hot-loader": "1.3.6",
    "css-loader": "0.28.0",
    "eslint": "4.16.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "node-sass": "4.5.2",
    "node-sass-json-importer": "3.1.2",
    "path": "0.12.7",
    "postcss-cssnext": "3.1.0",
    "postcss-loader": "2.0.10",
    "postcss-reporter": "5.0.0",
    "pre-commit": "1.2.2",
    "sass-loader": "6.0.3",
    "style-loader": "0.20.1",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.0.0",
    "stylelint-webpack-plugin": "0.10.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.4.2",
    "webpack-hot-middleware": "2.18.0"
  }
}
